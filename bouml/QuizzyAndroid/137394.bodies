            {
                traiterProtocoleEntrant(peripherique, protocole);
            }
        }
!!!150450.java!!!	traiterDeconnexion(inout msg : Message) : void
        Log.d(TAG, msg.obj.toString());
        boolean      estPrevue = (boolean)msg.obj;
        Peripherique peripherique =
