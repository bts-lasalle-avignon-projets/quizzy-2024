	  on state_ref 135082 // QuestionsAjoutees
	  external 
	end

	transition 135722 "Trame $I / ajouterParticipant(pidJoueur, nomParticipant)"
	  on state_ref 134954 // ParticipantsAjoutes
	end
      end

